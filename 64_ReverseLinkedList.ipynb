      "source": [
        "#O(n) time | O(n) space\n",
        "def reverseLinkedList(head):\n",
        "    p1,p2=None,head\n",
        "\twhile p2 is not None:\n",
        "\t\tp3=p2.next\n",
        "\t\tp2.next=p1\n",
        "\t\tp1=p2\n",
        "\t\tp2=p3\n",
        "\treturn p1"
      ],
      "execution_count": 0,
      "outputs": []
    }
  ]
